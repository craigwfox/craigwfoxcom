  "description": "Craig Fox, UX engineer / developer, maker, and geek",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "dev": "astro dev --host",
    "start": "astro dev --host",
    "build": "astro build && cp _redirects dist/_redirects",
    "preview": "astro preview",
    "astro": "astro",
    "convertCovers": "find ./public/images/covers -type f -name '*.jpg' -exec sh -c 'cwebp -q 75 $1 -o \"${1%.png}.webp\"';",
    "migrate:blog": "cp -R ~/Synced\\ Files/Notes\\ Vault/Blog/* ~/Development/Websites/Personal/craigwfoxcom/src/content/blog",
    "migrate:booksAll": "cp -R ~/Synced\\ Files/Notes\\ Vault/Media\\ -\\ Reading/Reading\\ Log/* ~/Development/Websites/Personal/craigwfoxcom/src/content/reading",
    "migrate:moveCovers": "rm -rf ~/Development/Websites/Personal/craigwfoxcom/public/images/covers && mv ~/Development/Websites/Personal/craigwfoxcom/src/content/reading/covers ~/Development/Websites/Personal/craigwfoxcom/public/images",
    "migrate:books": "npm run migrate:booksAll && npm run migrate:moveCovers",
    "migrate:hikes": "cp -R ~/Synced\\ Files/Notes\\ Vault/Hiking/Trail\\ Log/* ~/Development/Websites/Personal/craigwfoxcom/src/content/hiking",
    "migrate:full": "npm run migrate:books && npm run migrate:hikes && npm run migrate:blog"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/craigwfox/craigwfox.github.io.git"
  },
