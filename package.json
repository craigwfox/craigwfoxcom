    "@astrojs/vue": "^5.0.6",
    "@vitejs/plugin-vue": "^5.1.2",
    "astro": "^5.3.0",
    "markdown-it": "^14.1.0",
    "sass": "^1.75.0",
    "sharp": "^0.33.3",
    "simple-theme-switcher": "1.0.4",
    "vue": "^3.4.37"
  },
